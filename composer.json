{
    "name": "compwright/should-retry",
    "keywords": ["psr-7", "psr-15", "guzzle"],
    "description": "PSR interop HTTP retry middleware helper",
    "homepage": "https://github.com/compwright/should-retry",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2 || ^8.3 || ^8.4",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.72",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.6 || ^10.5 || 11.5 || ^12.0"
    },
    "autoload" : {
        "psr-4": {
            "Compwright\\ShouldRetry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compwright\\ShouldRetry\\": "tests/"
        }
    }
}
